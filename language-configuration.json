{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*#(function|if|for|while|ui)",
      "end": "^\\s*#end"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*#(function|if|elif|else|for|while|ui).*$",
    "decreaseIndentPattern": "^\\s*#end.*$"
  }
}
